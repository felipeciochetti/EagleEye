            <from-outcome>ProcessoView</from-outcome>
            <to-view-id>/usuario/ProcessoView.xhtml</to-view-id>
            <redirect>
            <view-param>
                    <name>id</name>
                    <value>#{proc.idProcesso}</value>                    
            </view-param>
            </redirect>
        </navigation-case>
        <navigation-case>

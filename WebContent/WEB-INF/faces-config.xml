<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    
    <navigation-rule>
    	<from-view-id>*</from-view-id>
    	<navigation-case>
            <from-outcome>ListaCliente</from-outcome>
            <to-view-id>/restrito/ListaCliente.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>AlterarCliente</from-outcome>
            <to-view-id>/restrito/CadastroCliente.xhtml</to-view-id>
            <redirect>
            <view-param>
                    <name>id</name>
                    <value>#{clie.idCliente}</value>                    
            </view-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>CadastroCliente</from-outcome>
            <to-view-id>/restrito/CadastroCliente.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

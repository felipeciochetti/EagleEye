            <to-view-id>/restrito/CadastroUsuario.xhtml</to-view-id>
            <redirect>
            <view-param>
                    <name>id</name>
                    <value>#{usu.idUsuario}</value>                    
            </view-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>CadastroUsuario</from-outcome>
            <to-view-id>/restrito/CadastroUsuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
            <navigation-case>
            <from-outcome>CadastroProcesso</from-outcome>
            <to-view-id>/restrito/CadastroProcesso.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>ListaProcesso</from-outcome>
            <to-view-id>/restrito/ListaProcesso.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>AlterarProcesso</from-outcome>
            <to-view-id>/restrito/CadastroProcesso.xhtml</to-view-id>
            <redirect>
            <view-param>
                    <name>id</name>
                    <value>#{proc.idProcesso}</value>                    
            </view-param>
            </redirect>
        </navigation-case>
            <navigation-case>
            <from-outcome>ListaProcessoView</from-outcome>
            <to-view-id>/usuario/ListaProcessoView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

        </navigation-case>
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/restrito/Login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        	<navigation-case>
            <from-outcome>ListaTransportadora</from-outcome>

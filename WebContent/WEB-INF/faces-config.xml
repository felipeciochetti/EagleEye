        </navigation-case>
        <navigation-case>
            <from-outcome>AlterarExportador</from-outcome>
            <to-view-id>/restrito/CadastroExportador.xhtml</to-view-id>
            <redirect>
            <view-param>
                    <name>id</name>
                    <value>#{exp.idExportador}</value>                    
            </view-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>CadastroExportador</from-outcome>
            <to-view-id>/restrito/CadastroExportador.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>AlterarNumerario</from-outcome>
            <to-view-id>/restrito/CadastroNumerario.xhtml</to-view-id>
            <redirect>
            <view-param>
                    <name>id</name>
                    <value>#{num.idNumerario}</value>                    
            </view-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>ListaNumerario</from-outcome>
            <to-view-id>/restrito/ListaNumerario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <lifecycle>
        <phase-listener>br.com.eagleeye.phase.MultiPageMessagesSupport</phase-listener>
    </lifecycle> 
</faces-config>
